<h1>Create Heroine: </h1><br>
<%= form_for(@heroine) do |f| %>
    <label><%= f.label :name %></label>
    <%= f.text_field :name %></br><br>
    <label><%= f.label :super_name %></label>
    <%= f.text_field :super_name %></br><br>
    <%= f.collection_select :power_id, Power.all, :id, :name, prompt:true %><br>
  <%= f.submit %><br>
<% end %>